{
  "name": "@cana-rails/type-spec",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "compile": "rimraf tsp-output && tsp compile .",
    "gen": "npm run gen-svr && npm run gen-web",
    "gen-svr": "nswag openapi2cscontroller /input:\"./tsp-output/@typespec/openapi3/openapi.yaml\" /namespace:CanaRails.Controllers /output:\"../Dashboard/Controllers/Controller.cs\" /ControllerBaseClass:\"Microsoft.AspNetCore.Mvc.Controller\"",
    "gen-web": "npx nswag openapi2tsclient /input:\"./tsp-output/@typespec/openapi3/openapi.yaml\" /output:\"../Dashboard.Web/api-client/index.ts\" /TypeStyle:Interface"
  },
  "dependencies": {
    "@types/node": "^20.13.0",
    "@typespec/compiler": "latest",
    "@typespec/http": "latest",
    "@typespec/openapi3": "latest",
    "@typespec/rest": "latest",
    "rimraf": "^5.0.7"
  },
  "private": true,
  "devDependencies": {
    "nswag": "^14.0.7"
  }
}
